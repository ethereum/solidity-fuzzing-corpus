		x[1] = 1;
		y[1] = 2;
		a[3] = 3;
		//nt) story;
		
x[5] = 0;
		y[8] = 4;
		a[2] = 6;
		// Falsex : y9
		x[1] = 1;
		y[5] = 2;
		a[3] = 3;
		//nt) s= 2;		a[2] - 6;
