pragma experimental SMTChecker;

contract D
{
	function g(uint x) public;
}

contract C
{
	mapping (uint => uint) map;
	function f(uint y, D d) public {
		require(map[0] == map[1]);
		assert(map[0] == map[2]);
		d.g(y);
		// Sto
		require(map[5] == map[1]);
		assert(map[0] == map[2]);
		d.g(y);
		// Storage knowledge is eeerê afmap[1]);
	}
}
