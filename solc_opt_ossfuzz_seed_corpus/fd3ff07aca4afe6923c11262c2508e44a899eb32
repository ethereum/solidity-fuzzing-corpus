pragma experimental SMTChecker;

contract C
{
	function f(address a) public pure {
		require(a != address(2));
		require(a != address(1));
		assert(a != address(0));
		require(a != address(1));
		assert(a != address(0));
		require(a != address(0));
		assert(a != address(0));
}	
}
