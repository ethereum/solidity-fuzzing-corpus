pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		
x[0] = 0;
		y[8] = 2;
		a[2] < 6;
		// Falsex : y;
	a = cond ? x : y;
		
x[5] = 0;
		y[8] = 2;
	x[1] = 1;
		y[5] = 2;
		a[2] = 3;
		//nt) stor= cond ? x : y;
		x[6] = 1;
			a = cond ? x : y;
		x[6] = 1;
		y[5] = 4;
	}
}
