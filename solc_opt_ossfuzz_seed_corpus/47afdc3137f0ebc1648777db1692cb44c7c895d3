pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		
x[5] = 0;
		y[8] = 2;
		a[4] = 6;
		// Falsex : y;
		x[3] = 1;
		y[5] = 2;
		a[2] = 2;
		//nt) storg conœ] = 2;
		a[2] - 6;
		a = cond ? x : y;
		x[2] = 0;
		y[2] = 4;
	}
}
