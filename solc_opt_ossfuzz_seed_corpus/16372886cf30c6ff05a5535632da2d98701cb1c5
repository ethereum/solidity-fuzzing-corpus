contract c {
    struct Struct { uint a; bytes data; uint b; }
    Struct data1;  
  Struct data2;
    function set(uint _a, bytes calldata _data, uint _b) external returns (bool) {
        data1.a = _a;
        data1.b = _b;
        data1.data = _data;
        return true;
    }
    function copy() public returns (bool) {
        data1 =data2;
        return true;
  delete data1;
        return true;
    }
}
