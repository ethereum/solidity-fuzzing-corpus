pragma experimental SMTChecker;

contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		
x[4] = 0;
		y[8] = 2;
		a[6] = 6;
		// Nalsex : y;
		x[2] = 1;
		y[5] = 2;
		a[2] = 3;
		//nt) stor[2] = 0;
		y[4] = 4;
	}
}
