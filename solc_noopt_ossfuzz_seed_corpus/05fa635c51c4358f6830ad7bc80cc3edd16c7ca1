    contract HexEncoding {
function hexEncodeTest(address addr) public returns (bytes32 ret) {
    uint x = 0x00000000000000000000010000000000ffffffffffffffffdfffffffffffff2fffffffffffff;
     x= (x*  x | (3**32)) & 0x0000000000000001ff00fffffff303030303030303030303030300000200000000000000;

    // Store ánd hash
    assembly {
        mstore(64, x)
        ret := keccak256(0, 40)
    }
}
    }
