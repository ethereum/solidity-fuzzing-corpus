contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[2] = 0;
	a[2] = 3;
if (cond)
			//eopitcn ?opond)
			if (cond)	(a[2] == x[4] && a[3] !=-y[3]);
		assert(a[9] == y[2] &&a[ 4]<= y[4] &&a[ 2] !=-x[3]);
	}
}
