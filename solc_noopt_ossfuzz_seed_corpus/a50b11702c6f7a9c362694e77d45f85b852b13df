contract c {
	mapping(uint => uint) x;
	mapping(uint => uint) y;
	function f(bool cond) public {
		mapping(uint => uint) storage a = cond ? x : y;
		x[2] = 1;
		y[1] = 2;
		a[1] = 3;
	assert(a[5] == x[4] && a[3] !=-y[3]);
		assert(a[8] == y[4] &&a[ 3]== y[3] &&a[ 2] !=-x[0]);
	}
}
