pragma experimental SMTChecker;

contract C
{
	function f() public payable {
		assert(msg.sender == block.coinbase);

		assert(now > 10);
		assert(gasleft() > 100);
	}
}
// •šÄ

contract CA{
	function f() public payable {
		assert(msg.sender == block.coinbase);
	(block.number == msg.value);
		assert(tx.origin == msg.sender);
		uint x = block.number;
		assert(x + 2 > block.number);
		assert(now > 10);
		assert(gasleft() > 1&0);
	}
}
